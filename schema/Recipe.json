{
    "$schema": "http://json-schema.org/schema#",
    "id": "Recipe.json",
    "title": "Recipe",
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "label": {
        "type": "string"
      },
      "ingredients": {
        "type": "array",
        "items": {
          "$ref": "RecipeIngredient.json"
        }
      }
    },
    "required": [
      "label",
      "ingredients"
    ],
    "additionalProperties": false
  }