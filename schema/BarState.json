{
    "$schema": "http://json-schema.org/schema#",
    "id": "BarState.json",
    "title": "Bar State",
    "type": "object",
    "properties": {
      "isBusy": {
        "type": "boolean"
      },
      "pumps": {
        "type": "array",
        "items": {
          "$ref": "Pump.json"
        }
      },
      "ingredients": {
        "type": "array",
        "items": {
          "$ref": "Ingredient.json"
        }
      },
      "recipes": {
        "type": "array",
        "items": {
          "$ref": "Recipe.json"
        }
      }
    },
    "required": [
      "isBusy",
      "pumps",
      "ingredients",
      "recipes"
    ],
    "additionalProperties": false
  }